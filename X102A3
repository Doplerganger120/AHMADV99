-- Script chính A
print("🔄 Đang chuẩn bị chạy RandomizerEGG...")

local success, err = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/quyhuynhu123/102TEAMS/refs/heads/main/RandomizerEGG"))()
end)

if not success then
    warn("❌ Gặp lỗi khi chạy RandomizerEGG:", err)
else
    print("✅ RandomizerEGG đã chạy xong.")
end
